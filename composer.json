{
  "name": "akki/sylius-payum-slimpay2-plugin",
  "description": "Payum Slimpay Bridge for Sylius",
  "type": "sylius-plugin",
  "require": {
    "sylius/sylius": "^1.5",
    "payum/core": "^1.3",
    "slimpay/hapiclient": "^1.0",
    "ext-json": "^1.6"
  },
  "require-dev": {
    "payum/core": "^1.3@dev",
    "php-http/guzzle6-adapter": "^1.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Akki",
      "email": "support@akki.fr"
    }
  ],
  "prefer-stable": false,
  "autoload": {
    "psr-4": {
      "Akki\\SyliusPayumSlimpayPlugin\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
