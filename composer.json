{
  "name": "akki/sylius-payum-slimpay2-plugin",
  "description": "Payum Slimpay Bridge for Sylius",
  "type": "sylius-plugin",
  "require": {
    "sylius/sylius": "^1.5",
    "payum/slimpay": "dev-master"
  },
  "repositories":[
    {
      "type": "git",
      "url" : "git@github.com:tumbochka/slimpay.git"
    }
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Akki",
      "email": "support@akki.fr"
    }
  ],
  "prefer-stable": false,
  "autoload": {
    "psr-4": {
      "Akki\\SyliusPayumSlimpayPlugin\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
